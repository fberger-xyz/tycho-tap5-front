{
    "name": "nextjs",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "sh -c 'NEXT_PUBLIC_COMMIT_TIMESTAMP=$(date +%s) next dev'",
        "build": "export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build",
        "start": "next start",
        "lint": "next lint",
        "eslint:fix": "eslint ./src --fix",
        "prettier:fix": "prettier --write src",
        "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
        "postinstall": "prisma generate",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:deploy": "prisma migrate deploy",
        "prisma:reset": "prisma migrate reset",
        "prisma:seed": "NODE_ENV=development prisma db seed",
        "prisma:studio": "prisma studio",
        "db:setup": "prisma generate && prisma migrate deploy",
        "vercel-build": "prisma generate && export NEXT_PUBLIC_COMMIT_TIMESTAMP=$(git log -1 --format=%ct) && next build"
    },
    "dependencies": {
        "@heroui/tooltip": "^2.2.26",
        "@next/env": "15.2.1",
        "@prisma/client": "^6.19.1",
        "@radix-ui/react-tabs": "^1.1.13",
        "@t3-oss/env-nextjs": "^0.13.10",
        "@tanstack/react-query": "^5.90.16",
        "@tanstack/react-virtual": "^3.13.17",
        "blo": "^1.2.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "connectkit": "^1.9.1",
        "date-fns": "^4.1.0",
        "dayjs": "^1.11.19",
        "echarts": "^5.6.0",
        "echarts-stat": "^1.2.0",
        "encoding": "^0.1.13",
        "ethers": "^5.8.0",
        "framer-motion": "^11.18.2",
        "graphql-request": "^7.4.0",
        "lucide-react": "^0.539.0",
        "next": "15.2.1",
        "numeral": "^2.0.6",
        "nuqs": "^2.8.6",
        "react": "19.0.0",
        "react-countdown-circle-timer": "^3.2.1",
        "react-dom": "19.0.0",
        "react-error-boundary": "^4.1.2",
        "react-hot-toast": "^2.6.0",
        "react-tooltip": "^5.30.0",
        "source-map-support": "^0.5.21",
        "superjson": "^2.2.6",
        "tailwind-merge": "^2.6.0",
        "tw-colors": "^3.3.2",
        "viem": "^2.43.5",
        "zod": "^4.3.5",
        "zustand": "^4.5.7"
    },
    "devDependencies": {
        "@iconify/react": "^5.2.1",
        "@next/eslint-plugin-next": "15.2.1",
        "@types/echarts": "^4.9.22",
        "@types/node": "^20.19.27",
        "@types/numeral": "^2.0.5",
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4",
        "@typescript-eslint/eslint-plugin": "^8.52.0",
        "@typescript-eslint/parser": "^8.52.0",
        "eslint": "^9.39.2",
        "eslint-config-next": "15.2.1",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-tailwindcss": "^3.18.2",
        "pino-pretty": "^13.1.3",
        "postcss": "^8.5.6",
        "prettier": "^3.7.4",
        "prettier-eslint": "^16.4.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "prisma": "^6.19.1",
        "sharp": "^0.34.5",
        "tailwindcss": "^3.4.19",
        "ts-prune": "^0.10.3",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    },
    "pnpm": {
        "overrides": {
            "@types/react": "19.0.10",
            "@types/react-dom": "19.0.4",
            "use-sync-external-store": "^1.2.2"
        }
    }
}
